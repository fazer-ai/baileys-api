    "/connections/{phoneNumber}": {
      "post": {
        "parameters": [
          {
            "description": "Phone number for connection. Must have + prefix.",
            "schema": {
              "type": "string",
              "minLength": 6,
              "maxLength": 16,
              "pattern": "^\\+\\d{5,15}$",
              "example": "+551234567890"
            },
            "in": "path",
            "name": "phoneNumber",
            "required": true
          }
        ],
        "operationId": "postConnectionsByPhoneNumber",
        "tags": [
          "Connections"
        ],
        "security": [
          {
            "xApiKey": []
          }
        ],
        "responses": {
          "200": {
            "description": "Connection initiated"
          }
        },
        "requestBody": {
          "required": true,
          "content": {
            "application/json": {
              "schema": {
                "type": "object",
                "properties": {
                  "clientName": {
                    "description": "Name of the client to be used on WhatsApp connection",
                    "example": "My WhatsApp Client",
                    "type": "string"
                  },
                  "webhookUrl": {
                    "format": "uri",
                    "description": "URL for receiving updates",
                    "example": "http://localhost:3026/whatsapp/+1234567890",
                    "type": "string"
                  },
                  "webhookVerifyToken": {
                    "minLength": 6,
                    "description": "Token for verifying webhook",
                    "example": "a3f4b2",
                    "type": "string"
                  },
                  "options": {
                    "type": "object",
                    "properties": {
                      "includeMedia": {
                        "description": "Include media in messages.upsert event payload as base64 string",
                        "default": true,
                        "type": "boolean"
                      }
                    }
                  }
                },
                "required": [
                  "webhookUrl",
                  "webhookVerifyToken"
                ]
              }
            },
            "multipart/form-data": {
              "schema": {
                "type": "object",
                "properties": {
                  "clientName": {
                    "description": "Name of the client to be used on WhatsApp connection",
                    "example": "My WhatsApp Client",
                    "type": "string"
                  },
                  "webhookUrl": {
                    "format": "uri",
                    "description": "URL for receiving updates",
