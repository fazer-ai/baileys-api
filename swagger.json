  },
  "servers": [
    {
      "url": "http://localhost:3025",
      "description": "Local development server"
    },
    {
      "url": "{scheme}://{customUrl}",
      "description": "Custom server",
      "variables": {
        "scheme": {
          "enum": [
            "http",
            "https"
          ],
          "default": "https",
          "description": "HTTP or HTTPS"
        },
        "customUrl": {
          "default": "your-domain.com",
          "description": "Your API domain (without protocol)"
        }
      }
    }
  ],
